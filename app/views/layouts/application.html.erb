<!DOCTYPE html>
<html>
  <head>
    <title>DishesHub</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'http://fonts.googleapis.com/icon?family=Material+Icons' %>
  </head>

  <body>

    <nav>
    <div class="nav-wrapper">
      <%= link_to root_path, { class: "brand-logo center" } do %>
       <i class="material-icons">room_service</i> Dishes Hub
      <% end %>
      <ul class="left hide-on-med-and-down">
        <li><%= link_to "Categories", categories_path %></li>
        <li><a href="badges.html">About us</a></li>
        <li class="active"><a href="collapsible.html">Contacts</a></li>
      </ul>
      <ul class="right hide-on-med-and-down">
       <li><a href="sass.html"><i class="material-icons">search</i></a></li>
       <li><a href="badges.html"><i class="material-icons">view_module</i></a></li>
       <li><a href="collapsible.html"><i class="material-icons">refresh</i></a></li>
       <li><a href="mobile.html"><i class="material-icons">more_vert</i></a></li>

     </ul>
    </div>
  </nav>
 </nav>
 <div class="container">
      <p class="notice"><%= notice %></p>
      <p class="alert"><%= alert %></p>
    <%= yield %>
  </div>

  </body>
</html>
